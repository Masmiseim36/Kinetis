<!DOCTYPE CrossStudio_Group_Values>
<propertyGroups>
  <group short="Target Processor" group="Build Options" long="Select a set of target options" name="Target" default="MK60N512VMD100">
    <groupEntry name="MK10N512VMD100" family="K10">
      <property value="$(TargetsDir)/Kinetis/MK10N512VMD100_MemoryMap.xml" name="linker_memory_map_file" />      
      <cdefine value="MK10N512VMD100" name="__TARGET_PROCESSOR" />         
      <property name="arm_simulator_memory_simulation_parameter" value="MK10N512VMD100;0x80000;0;0;0x20000"/>
      <property name="arm_target_flash_loader_file_path" configuration="Flash" value="$(TargetsDir)/Kinetis/N512/Loader.elf" />
    </groupEntry> 
